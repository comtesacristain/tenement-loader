<tenement-mapping state="NSW">
	<field target="name" source="TENID" type="string" />
	<field target="tenementType" source="TYPE" type="string" />
	<field target="owner" source="OWNER" type="string" />
	<field target="status" source="TENSTATUS" type="string" />
	<field target="area" source="AREA" type="string" />
	<field target="applicationDate" source="APPDATE" type="date"
		format="yyyyMMdd" />
	<field target="grantDate" source="GRANTDATE" type="date" format="yyyyMMdd" />
	<field target="expireDate" source="EXPIREDATE" type="date"
		format="yyyyMMdd" />
	<field target="tenementType_uri" source="TYPE" type="uri" uri="http://resource.geoscience.gov.au/classifier/ggic/tenementtype/">
		<mappings>
			<mapping>
				<source>Assessment lease</source>
				<target>assessment lease</target>
			</mapping>
			<mapping>
				<source>Exploration licence</source>
				<target>exploration licence</target>
			</mapping>
			<mapping>
				<source>Mining lease</source>
				<target>mining lease</target>
			</mapping>
		</mappings>
	</field>
	<field target="status_uri" source="TENSTATUS" type="uri" uri="http://resource.geoscience.gov.au/classifier/ggic/tenement-status/">
		<mappings>
			<mapping>
				<source>LIVE</source>
				<target>live</target>
			</mapping>
			<mapping>
				<source>PENDING</source>
				<target>pending</target>
			</mapping>
		</mappings>
	</field>
	<field target="jurisdiction_uri" type="uri"
		uri="http://www.resourcesandenergy.nsw.gov.au/miners-and-explorers/applications-and-approvals/current-titles-reports-applications-and-services" />
	<field target="geom" source="the_geom" type="geometry" />
</tenement-mapping>