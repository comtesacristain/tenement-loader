<tenement-mapping state="QLD">
	<field target="name" source="TENID" type="string" />
	<field target="tenementType" source="TYPE" type="vocabulary">
		<mappings>
			<mapping>
				<source>ML</source>
				<target>mining lease</target>
			</mapping>
			<mapping>
				<source>EPC</source>
				<target>exploration permit for coal</target>
			</mapping>
			<mapping>
				<source>EPM</source>
				<target>exploration permit for minerals</target>
			</mapping>
		</mappings>
	</field>
	<field target="owner" source="OWNER" type="string" />
	<field target="status" source="TENSTATUS" type="vocabulary">
		<mappings>
			<mapping>
				<source>Granted</source>
				<target>granted</target>
			</mapping>
			<mapping>
				<source>Application</source>
				<target>application</target>
			</mapping>
		</mappings>
	</field>
	<field target="area" source="AREA" type="string" />
	<field target="applicationDate" source="APPDATE" type="date"
		format="yyyy-MM-dd" />
	<field target="grantDate" source="GRANTDATE" type="date" format="yyyy-MM-dd" />
	<field target="expireDate" source="EXPIREDATE" type="date"
		format="yyyy-MM-dd" />
	<field target="geom" source="the_geom" type="geometry" />
</tenement-mapping>